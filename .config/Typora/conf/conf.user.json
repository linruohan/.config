/** For advanced users. */
{
    "defaultFontFamily": {
        "standard": "PingFang SC", //String - Defaults to "Times New Roman"."Consolas"
        "serif": "PingFang SC", // String - Defaults to "Times New Roman".
        "sansSerif": "PingFang SC", // String - Defaults to "Arial".
        "monospace": "PingFang SC" // String - Defaults to "Courier New".
    },
    "autoHideMenuBar": true, //Boolean - Auto hide the menu bar unless the `Alt` key is pressed. Default is false.

    // Array - Search Service user can access from context menu after a range of text is selected. Each item is formatted as [caption, url]
    "searchService": [
        ["Search with Google", "https://google.com/search?q=%s"],
        ["Translate", "http://translate.google.com/?source=osdd#auto|auto|%s"],
        ["Search with Wikipedia", "https://zh.wikipedia.org/wikipedia.org/wiki/Special:Search/%s"]
    ],

    // Custom key binding, which will override the default ones.
    "keyBinding": {
        // for example: 
        // "Always on Top": "Ctrl+Shift+P"
        "Highlight": "Ctrl + Q",
        "Delete line/Sentence": "Ctrl + x",
		"Unordered List": "Ctrl + Shfit + U"
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q",
            // "Highlight": "Ctrl + q"
    },

    "monocolorEmoji": true, //default false. Only work for Windows
    "autoSaveTimer": 1, // Deprecidated, Typora will do auto save automatically. default 3 minutes
    "maxFetchCountOnFileList": 500,
    "flags": [] // default [], append Chrome launch flags, e.g: [["disable-gpu"], ["host-rules", "MAP * 127.0.0.1"]]
}
